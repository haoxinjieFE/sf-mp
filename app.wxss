/* 引入 we-ui */
@import "style/weui.wxss";
/* 引入 templates */
@import "templates/template.wxss";
/* 引入 include */
@import "include/include.wxss";
/* font */

@import "style/icon.wxss";

@import "templates/common/common.wxss";

/* 基础样式库 */
page {
  min-height: 100%;
  background-color: #FAFAFA;
}

button {
  border-radius: 0;
}

button::after {
  border: none;
}

view, scroll-view {
  box-sizing: border-box;
}

/** app.wxss **/

/* 空 样式 */
.empty_box {
  text-align: center;
  padding-top: 250rpx;
}

.empty_box.cart {
  padding-top: 350rpx;
}

.empty_box image {
  width: 200rpx;
  height: 200rpx;
}

.empty_box_text {
  font-size: 32rpx;
  color: #BBB;
}



