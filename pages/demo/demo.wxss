@-webkit-keyframes rotate {
    form {
        transform: rotateZ(0deg)
    }
    to {
        transform: rotateZ(359deg) 
    }
}

.container {
    display: flex;
    width: 100%;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.demo {
    width: 200rpx;
    height: 200rpx;
    margin-top: 100rpx;
    border-radius: 50%;
    animation: rotate 3s infinite linear;
}

.words {
    margin-top: 20rpx;
    font-size: 50rpx;
    color: #969696;
    text-align: center;
}